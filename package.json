{
	"name": "freshman-tool",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"move-pdfjs": "cp -a node_modules/@pdftron/pdfjs-express-viewer/public/. ./static/pdfjs-express",
		"build": "vite build && npm run move-pdfjs",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"vercel-build": "npm install --legacy-peer-deps && npm run build"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.3",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/youtube": "^0.1.2",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"zod": "^4.1.12"
	},
	"dependencies": {
		"@google/generative-ai": "^0.24.1",
		"@pdftron/pdfjs-express-viewer": "^8.7.5",
		"@supabase/supabase-js": "^2.58.0",
		"cookie-parser": "^1.4.7",
		"dotenv": "^17.2.1",
		"lottie-web": "^5.13.0",
		"pdfjs-dist": "^5.4.54",
		"svelte-pdf-simple": "^2.0.0",
		"svelte-youtube-lite": "^1.2.1",
		"telegraf": "^4.16.3"
	}
}
